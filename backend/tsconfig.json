{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"moduleResolution": "node",
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"outDir": "./build",
		"noImplicitAny": true
	},
	"include": ["./src", ".env"]
}
